# Description : A frontend to ssh-agent and ssh-add
# URL         : https://www.funtoo.org/keychain
# Maintainer  : camille, camille at lzr dot pw

name=keychain
version=2.9.5
release=1
source=(
"https://github.com/funtoo/$name/releases/download/2.9.2/$name"
"https://github.com/funtoo/$name/releases/download/2.9.2/$name.1"
)

build() {
    install -Dm755 "$name"   -t "$PKG/usr/bin"
    install -Dm644 "$name.1" -t "$PKG/usr/share/man/man1"
}
